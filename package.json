{
  "name": "@tiledb-inc/tiledb-cloud",
  "version": "1.0.9",
  "description": "JavaScript client for the TileDB Cloud Service.",
  "main": "lib/index.js",
  "module": "lib/module.js",
  "files": [
    "/lib/**/*"
  ],
  "scripts": {
    "generate-openapi:v1": "openapi-generator-cli generate -i ../TileDB-Cloud-API-Spec/openapi-v1.yaml -o ./src/v1 -g typescript-axios -t ./openapi-templates --reserved-words-mappings default=default --type-mappings=Array=any",
    "generate-openapi:v2": "openapi-generator-cli generate -i ../TileDB-Cloud-API-Spec/openapi-v2.yaml -o ./src/v2 -g typescript-axios -t ./openapi-templates --reserved-words-mappings default=default --type-mappings=Array=any",
    "build": "npm run generate-openapi:v1 && npm run generate-openapi:v2 && npm run build-ts",
    "clean": "rm -r lib",
    "build-ts": "tsc && npm run build-ts:module",
    "build-ts:module": "node ./scripts/esbuild.js",
    "test": "jest",
    "capnp": "capnpc -o ts src/capnp/arrayMetadata.capnp && capnpc -o ts src/capnp/query.capnp"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "capnp-ts": "^0.4.0",
    "capnpc-ts": "^0.4.0",
    "paralleljs": "github:SarantopoulosKon/parallel.js#node-mode-in-web-worker",
    "save-file": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@openapitools/openapi-generator-cli": "^2.1.26",
    "@types/jest": "^27.0.1",
    "axios-mock-adapter": "^1.20.0",
    "esbuild": "^0.15.7",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.2.3"
  },
  "author": "",
  "license": "MIT"
}
