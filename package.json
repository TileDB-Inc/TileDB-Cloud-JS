{
  "name": "@tiledb-inc/tiledb-cloud",
  "version": "1.0.12-alpha.2",
  "description": "JavaScript client for the TileDB Cloud Service.",
  "main": "lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "generate-openapi:v1": "openapi-generator-cli generate -i ../TileDB-Cloud-API-Spec/openapi-v1.yaml -o ./src/v1 -g typescript-axios -t ./openapi-templates --reserved-words-mappings default=default --type-mappings=Array=any",
    "generate-openapi:v2": "./scripts/update_yaml_values.sh && openapi-generator-cli generate -i ../TileDB-Cloud-API-Spec/openapi-v2.yaml -o ./src/v2 -g typescript-axios -t ./openapi-templates --reserved-words-mappings default=default --type-mappings=Array=any",
    "build": "npm run generate-openapi:v1 && npm run generate-openapi:v2 && tsc",
    "build-ts": "tsc",
    "test": "jest",
    "capnp": "capnpc -o ts src/capnp/arrayMetadata.capnp && capnpc -o ts src/capnp/query.capnp"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "capnp-ts": "^0.4.0",
    "capnpc-ts": "^0.4.0",
    "paralleljs": "github:SarantopoulosKon/parallel.js#node-mode-in-web-worker",
    "save-file": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@openapitools/openapi-generator-cli": "^2.1.26",
    "@types/jest": "^27.0.1",
    "axios-mock-adapter": "^1.20.0",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.2.3"
  },
  "overrides": {
    "capnp-ts": {
      "tslib": "1.14.1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TileDB-Inc/TileDB-Cloud-JS.git"
  },
  "author": "",
  "license": "MIT"
}
